{
  "name": "tech-challenge",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "api": "yarn workspace @tech-challenge/api",
    "web": "yarn workspace @tech-challenge/web",
    "build": "turbo run build",
    "dev": "concurrently \"yarn api start:dev\" \"yarn web dev\"",
    "start": "yarn api start:prod"
  },
  "workspaces": [
    "packages/*"
  ],
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "concurrently": "^7.2.2",
    "eslint": "^8.0.1",
    "eslint-config-next": "^14.1.3",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.3.2",
    "turbo": "^1.3.1",
    "typescript": "^4.3.5"
  }
}
